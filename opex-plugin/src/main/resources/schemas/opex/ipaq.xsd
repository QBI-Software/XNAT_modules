<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2017 rel. 3 (x64) (http://www.altova.com) by e.cooperwilliams (The University of Queensland) -->
<!-- OPEX datatype for Exercise tests-->
<xs:schema xmlns:opex="http://qbi.uq.edu.au/opex" xmlns:xnat="http://nrg.wustl.edu/xnat" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://qbi.uq.edu.au/opex" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:import namespace="http://nrg.wustl.edu/xnat" schemaLocation="../../xnat/xnat.xsd"/>
	
	<xs:element name="IPAQ" type="opex:ipaq"/>
	<xs:complexType name="ipaq">
		<xs:annotation>
			<xs:documentation>International Physical Activity Questionnaire (IPAQ)</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="xnat:subjectAssessorData">
				<xs:sequence>
				<xs:element name="interval" type="xs:integer" default="0">
				<xs:annotation>
					<xs:documentation>
                                Monthly interval stage
                            </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sample_id" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
								Sample ID from analyser
							</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sample_quality" default="Unknown">
				<xs:annotation>
					<xs:documentation>
								Sample Quality
							</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Good"/>
						<xs:enumeration value="Questionable"/>
						<xs:enumeration value="Poor"/>
						<xs:enumeration value="Unknown"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="data_valid" default="Initial">
				<xs:annotation>
					<xs:documentation>
								Data validation check
							</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Initial"/>
						<xs:enumeration value="Checked"/>
						<xs:enumeration value="Corrected"/>
						<xs:enumeration value="Invalid"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="comments" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
								Comments
							</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
					<xs:element name="pa" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Physical activity (mins/week)
							</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:float">
								<xs:minInclusive value="0"/>
								<xs:maxInclusive value="10080"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="mvpa" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								MV Physical activity (mins/week)
							</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:float">
								<xs:minInclusive value="0"/>
								<xs:maxInclusive value="10080"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="sitting" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Last 7 days time spent sitting (mins/week)
							</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:float">
								<xs:minInclusive value="0"/>
								<xs:maxInclusive value="10080"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="walking_days" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Last 7 days, days spent walking more than 10 mins (days)
							</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:integer">
								<xs:minInclusive value="0"/>
								<xs:maxInclusive value="7"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="walking_time" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Last 7 days time spent walking total (mins/week)
							</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:float">
								<xs:minInclusive value="0"/>
								<xs:maxInclusive value="10080"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="moderate_days" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Last 7 days, days doing moderate activities (days)
							</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:integer">
								<xs:minInclusive value="0"/>
								<xs:maxInclusive value="7"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="moderate_time" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Last 7 days time doing moderate activities (mins/week)
							</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:float">
								<xs:minInclusive value="0"/>
								<xs:maxInclusive value="10080"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="vigorous_days" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Last 7 days, days doing vigorous activities (days)
							</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:integer">
								<xs:minInclusive value="0"/>
								<xs:maxInclusive value="7"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="vigorous_time" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
								Last 7 days time doing vigorous activities (mins/week)
							</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:float">
								<xs:minInclusive value="0"/>
								<xs:maxInclusive value="10080"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
</xs:schema>
