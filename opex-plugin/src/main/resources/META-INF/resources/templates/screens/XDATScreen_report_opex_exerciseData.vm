	<td>$interval.get($!item.getIntegerProperty("$om.getXSIType()/interval"))</td>
</tr>
<tr>
	<td>Sample ID</td>
	<td>$!item.getStringProperty("$om.getXSIType()/sample_id")</td>
</tr>

<tr>
    <td>Data Quality</td>
    <td>$!item.getStringProperty("$om.getXSIType()/sample_quality")</td>
</tr>
