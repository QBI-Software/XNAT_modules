        <div class="panel-heading">Medical History</div>
        <div class="panel-body">


            <table class="table table-striped">
                <thead>
                <tr>
                    <th>Field</th>
                    <th>Value</th>
                </tr>
                </thead>
                <tbody>
                <tr>
                    <td>Vision Problems</td>
                    <!-- xnat:subjectData/demographics[@xsi:type='irc5:medicalData']/VisionProblems -->
