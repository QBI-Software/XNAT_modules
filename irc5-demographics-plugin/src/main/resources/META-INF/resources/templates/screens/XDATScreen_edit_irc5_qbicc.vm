                </tr>
                <tr>
                    <td>Auditory Problems</td>
                    <!-- xnat:subjectData/demographics[@xsi:type='irc5:medicalData']/VisionProblems -->
                    <td>#xdatRadioButtonGroup("xnat:subjectData/demographics[@xsi:type='irc5:medicalData']/AuditoryProblems" $item $yesno $vr)</td>
                </tr>
                </tbody>
            </table>

        </div>
    </div>
    <div class="panel panel-default">
        <div class="panel-heading">Birth History</div>
        <div class="panel-body">
            <p>parse BirthHistory screen</p>
        </div>
    </div>

</div>
