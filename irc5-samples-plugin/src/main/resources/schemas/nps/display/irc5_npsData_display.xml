    <ViewLink alias="IRC5_NPSDATA_PROJECTS">
        <Mapping TableName="IRC5_NPSDATA_PROJECTS">
            <MappingColumn rootElement="irc5:npsData" fieldElement="irc5:npsData.ID" mapsTo="id"/>
