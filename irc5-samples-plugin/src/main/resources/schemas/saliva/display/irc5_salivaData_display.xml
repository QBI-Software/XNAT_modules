		<Content type="sql">irc5_salivadata_project_id</Content>
		<HTML-Link>
			<SecureLink elementName="irc5:salivaData">
				<securityMappingValue displayFieldId="PROJECTS" schemaElementMap="irc5:salivaData/sharing/share/project"/>
				<securityMappingValue displayFieldId="PROJECT" schemaElementMap="irc5:salivaData/project"/>
			</SecureLink>
			<Property name="HREF" value="@WEBAPPapp/action/DisplayItemAction/search_value/@Field1/search_element/irc5:salivaData/search_field/irc5:salivaData.ID/project/@Field2">
				<InsertValue id="Field1" field="EXPT_ID"/>
				<InsertValue id="Field2" field="@WHERE"/>
			</Property>
		</HTML-Link>
		<SubQuery>SELECT DISTINCT COALESCE(label,sharing_share_xnat_experimentda_id) AS irc5_salivadata_project_id,sharing_share_xnat_experimentda_id FROM (	SELECT sharing_share_xnat_experimentda_id,label FROM xnat_experimentdata_share WHERE project='@WHERE'	UNION 	SELECT id,label FROM xnat_experimentData WHERE project='@WHERE' )SEARCH</SubQuery>
		<MappingColumns>
			<MappingColumn schemaField="irc5:salivaData.ID" queryField="sharing_share_xnat_experimentda_id"/>
		</MappingColumns>
	</DisplayField>
