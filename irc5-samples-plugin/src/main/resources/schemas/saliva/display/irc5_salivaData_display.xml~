            </SecureLink>
            <Property name="HREF"
                      value="@WEBAPPapp/action/DisplayItemAction/search_value/@Field1/search_element/irc5:salivaData/search_field/irc5:salivaData.ID">
                <InsertValue id="Field1" field="EXPT_ID"/>
            </Property>
            <Property name="ONCLICK" value="return rpt('@Field1','irc5:salivaData','irc5:salivaData.ID');">
                <InsertValue id="Field1" field="EXPT_ID"/>
