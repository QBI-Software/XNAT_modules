    <DisplayField id="IRC5_SALIVADATA_PROJECT_IDENTIFIER" header="IRC5_SALIVADATA ID" visible="true" searchable="false"
                  data-type="string" xsi:type="SubQueryField">
        <Content type="sql">irc5_salivadata_project_id</Content>
        <HTML-Link>
            <SecureLink elementName="irc5:salivaData">
                <securityMappingValue displayFieldId="PROJECTS"
                                      schemaElementMap="irc5:salivaData/sharing/share/project"/>
                <securityMappingValue displayFieldId="PROJECT" schemaElementMap="irc5:salivaData/project"/>
            </SecureLink>
            <Property name="HREF"
                      value="@WEBAPPapp/action/DisplayItemAction/search_value/@Field1/search_element/irc5:salivaData/search_field/irc5:salivaData.ID/project/@Field2">
                <InsertValue id="Field1" field="EXPT_ID"/>
                <InsertValue id="Field2" field="@WHERE"/>
            </Property>
        </HTML-Link>
        <SubQuery>SELECT DISTINCT COALESCE(label,sharing_share_xnat_experimentda_id) AS
            irc5_salivadata_project_id,sharing_share_xnat_experimentda_id FROM ( SELECT
            sharing_share_xnat_experimentda_id,label FROM xnat_experimentdata_share WHERE project='@WHERE' UNION SELECT
            id,label FROM xnat_experimentData WHERE project='@WHERE' )SEARCH
        </SubQuery>
        <MappingColumns>
            <MappingColumn schemaField="irc5:salivaData.ID" queryField="sharing_share_xnat_experimentda_id"/>
        </MappingColumns>
    </DisplayField>
    <DisplayField id="INSERT_DATE" header="Inserted" visible="true" searchable="true" data-type="string">
        <DisplayFieldElement name="Field1" schema-element="irc5:salivaData.meta.insert_date"/>
    </DisplayField>
    <DisplayField id="INSERT_USER" header="Creator" visible="true" searchable="true" data-type="string">
        <DisplayFieldElement name="Field1" schema-element="irc5:salivaData.meta.insert_user.login"/>
    </DisplayField>
    <DisplayField id="IRC5_SALIVADATA_FIELD_MAP" header="Field" visible="true" searchable="false" data-type="string"
                  xsi:type="SubQueryField">
        <Content type="sql">field</Content>
        <SubQuery>SELECT DISTINCT ON ( e.ID) e.ID AS expt_id,field FROM xnat_experimentData_field ef JOIN (SELECT
            ID,extension,element_name FROM xnat_experimentData e JOIN xdat_meta_element xme ON
            e.extension=xme.xdat_meta_element_id WHERE xme.element_name='irc5:salivaData') e on
            ef.fields_field_xnat_experimentdat_id=e.id WHERE name='@WHERE'
        </SubQuery>
        <MappingColumns>
            <MappingColumn schemaField="irc5:salivaData.ID" queryField="expt_id"/>
        </MappingColumns>
    </DisplayField>
