    <ViewLink alias="IRC5_SALIVADATA_PROJECTS">
        <Mapping TableName="IRC5_SALIVADATA_PROJECTS">
            <MappingColumn rootElement="irc5:salivaData" fieldElement="irc5:salivaData.ID" mapsTo="id"/>
        </Mapping>
    </ViewLink>
    <SQLView name="IRC5_SALIVADATA_PROJECTS"
             sql="SELECT id, '&lt;' || expt.project || '&gt;' || xs_a_concat(',&lt;' || shared.project || '&gt;') AS projects FROM xnat_experimentData expt LEFT JOIN xnat_experimentData_share shared ON expt.id=shared.sharing_share_xnat_experimentda_id LEFT JOIN xdat_meta_element xme ON expt.extension = xme.xdat_meta_element_id WHERE element_name='irc5:salivaData' GROUP BY expt.id,expt.project"/>
