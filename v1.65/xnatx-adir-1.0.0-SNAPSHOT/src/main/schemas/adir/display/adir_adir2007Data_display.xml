	<ViewLink alias="ADIR_ADIR2007DATA_PROJECTS">
		<Mapping TableName="ADIR_ADIR2007DATA_PROJECTS">
			<MappingColumn rootElement="adir:adir2007Data" fieldElement="adir:adir2007Data.ID" mapsTo="id"/>
		</Mapping>
	</ViewLink>
	<SQLView name="ADIR_ADIR2007DATA_PROJECTS" sql="SELECT id, '&lt;' || expt.project || '&gt;' || xs_a_concat(', ' || shared.project) AS projects FROM xnat_experimentData expt LEFT JOIN xnat_experimentData_share shared ON expt.id=shared.sharing_share_xnat_experimentda_id LEFT JOIN xdat_meta_element xme ON expt.extension = xme.xdat_meta_element_id WHERE element_name='adir:adir2007Data' GROUP BY expt.id,expt.project"/>
